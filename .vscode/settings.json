{
    "yaml.schemas": {
        "./schemas/dbt_schema.json": [
            "/*.yml",
            "!profiles.yml",
            "!dbt_project.yml",
            "!packages.yml"
        ],
        "./schemas/dbt_project.json": [
            "dbt_project.yml"
        ],
        "./schemas/packages.json": [
            "packages.yml"
        ]
    },
    "sortJSON.orderOverride": [
        "version",
        "title",
        "name",
        "type",
        "required",
        "if",
        "then",
        "else"
    ],
    "sortJSON.orderUnderride": [
        "properties",
        "patternProperties",
        "additionalProperties",
        "$defs"
    ]
}