{
    "title": "dbt_project",
    "type": "object",
    "additionalProperties": false,
    "required": [
        "name",
        "version",
        "config-version"
    ],
    "properties": {
        "name": {
            "type": "string"
        },
        "version": {
            "type": "string"
        },
        "config-version": {
            "type": "number",
            "default": 2
        },
        "profile": {
            "type": "string"
        },
        "model-paths": {
            "$ref": "#/$defs/array_of_strings"
        },
        "analysis-paths": {
            "$ref": "#/$defs/array_of_strings"
        },
        "test-paths": {
            "$ref": "#/$defs/array_of_strings"
        },
        "seed-paths": {
            "$ref": "#/$defs/array_of_strings"
        },
        "macro-paths": {
            "$ref": "#/$defs/array_of_strings"
        },
        "snapshot-paths": {
            "$ref": "#/$defs/array_of_strings"
        },
        "target-path": {
            "type": "string"
        },
        "clean-targets": {
            "$ref": "#/$defs/array_of_strings"
        },
        "models": {
            "$ref": "#/$defs/model_configs"
        },
        "tests": {
            "$ref": "#/$defs/test_configs"
        }
    },
    "$defs": {
        "array_of_strings": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "string_or_array_of_strings": {
            "oneOf": [
                {
                    "type": "string"
                },
                {
                    "$ref": "#/$defs/array_of_strings"
                }
            ]
        },
        "model_configs": {
            "title": "Model configs",
            "description": "Configurations set in the dbt_project.yml file will apply to all models that don't have a more specific configuration set. Nested subdirectories are supported.",
            "type": "object",
            "properties": {
                "+materialized": {
                    "type": "string"
                },
                "+sql_header": {
                    "type": "string"
                },
                "+enabled": {
                    "type": "boolean"
                },
                "+tags": {
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        }
                    ]
                }
            },
            "additionalProperties": {
                "$ref": "#/$defs/model_configs"
            }
        },
        "test_configs": {
            "title": "Test configs",
            "description": "Configurations set in the dbt_project.yml file will apply to all tests that don't have a more specific configuration set. Nested subdirectories are supported.",
            "type": "object",
            "properties": {
                "+enabled": {
                    "type": "boolean"
                },
                "+database": {
                    "description": "Only relevant when `store_failures` is true",
                    "type": "string"
                },
                "+schema": {
                    "description": "Only relevant when `store_failures` is true",
                    "type": "string"
                },
                "+alias": {
                    "description": "Only relevant when `store_failures` is true",
                    "type": "string"
                },
                "+store_failures": {
                    "type": "boolean"
                },
                "+fail_calc": {
                    "type": "string"
                },
                "+limit": {
                    "type": "number"
                },
                "+severity": {
                    "type": "string",
                    "enum": [
                        "warn",
                        "error"
                    ]
                },
                "+error_if": {
                    "type": "string"
                },
                "+warn_if": {
                    "type": "string"
                },
                "+tags": {
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        }
                    ]
                }
            },
            "additionalProperties": {
                "$ref": "#/$defs/test_configs"
            }
        }
    }
}