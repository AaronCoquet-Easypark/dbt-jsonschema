{
    "title": "dbt_yml_files",
    "type": "object",
    "required": [
        "version"
    ],
    "properties": {
        "version": {
            "type": "number"
        },
        "models": {
            "type": "array",
            "items": {
                "type": "object",
                "required": [
                    "name"
                ],
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "columns": {
                        "type": "array",
                        "items": {
                            "$ref": "#/$defs/column_properties"
                        }
                    },
                    "config": {
                        "type": "object"
                    },
                    "description": {
                        "type": "string"
                    }
                },
                "additionalProperties": false
            }
        }
    },
    "additionalProperties": false,
    "$defs": {
        "column_properties": {
            "type": "object",
            "required": "name",
            "properties": {
                "name": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                },
                "tests": {
                    "type": "array",
                    "items": {
                        "$ref": "#/$defs/tests"
                    }
                }
            },
            "additionalProperties": false
        },
        "tests": {
            "oneOf": [
                {
                    "type": "string"
                },
                {
                    "type": "object",
                    "properties": {
                        "config": {
                            "type": "object"
                        }
                    }
                }
            ]
        }
    }
}